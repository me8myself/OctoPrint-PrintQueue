{% extends "base.jinja2" %}

{% block content %}
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Print Queue</h3>
  </div>
  <div class="panel-body">
    <div class="row">
      <div class="col-md-6">
        <button class="btn btn-danger" onclick="clearQueue()">Clear Queue</button>
      </div>
      <div class="col-md-6 text-right">
        <button class="btn btn-primary" data-toggle="modal" data-target="#addModal">Add to Queue</button>
      </div>
    </div>
    <br>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Model Name</th>
          <th>Color</th>
          <th>User Name</th>
        </tr>
      </thead>
      <tbody id="queueBody">
      </tbody>
    </table>
  </div>
</div>

